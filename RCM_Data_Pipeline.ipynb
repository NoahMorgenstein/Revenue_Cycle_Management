    "user = os.getenv(\"DB_USER\")\n",
    "password = os.getenv(\"DB_PASSWORD\")\n",
    "\n",
    "print(\"Host:\", host)\n",
    "print(\"Database:\", database)\n",
    "print(\"User:\", user)"
